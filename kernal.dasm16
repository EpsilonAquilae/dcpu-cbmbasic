                JMP ROMAAF8
            SET C, 0
            IFE A, BASIC_SPC_TOKEN   ; SPC(
                JMP ROMAAF8
            IFE A, CBM_COMMA
                JMP ROMAAE8
            IFE A, 0x3B ; SEMICOLON
                JMP ROMAB13
            JSR FRMEVAL
            IFB [VALTYP], 0x80 ; number/variable
                JMP ROMAA9A
            JSR FOUT    ; store floating point number
            JSR STRLIT
            JSR ROMAB21 ; print string in [INDEX]
            JSR ROMAB3B
            IFN A, 0
                JMP ROMAA9D
            
; zero-terminate this string
:ROMAACA    SET [RAM0200+X], 0
            ; return location of input buffer in X
            SET X, RAM0200-1
            SET Z, 0
            IFN [CHANNL], 0
                RTS
            SET Z, 1
:ROMAAD7 ; end line on CMD output file
        SET A, CBM_RETURN  
        JSR ROMAB47
        IFC [CHANNL], 0x80
            JMP ROMAAE5
        SET A, 0x0A  ; newline
            JSR ROMAB47

:ROMAAE5    BOR A, 0xFF 

; todo: verify what X and Y are upon returning from this
:ROMAAE7          RTS

