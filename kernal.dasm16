            ; SET [I+1], 0 if it's unconnected
            ADD I, 2
            SET [VARTAB], I

:ROMA659    JSR ROMA68E ; set RAM7A to [TXTTAB]-1

:ROMA65C    SET A, 0     ;LDA #$00
            SET Z, 1
