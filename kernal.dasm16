:EVAL
:ROMAE83    JMP [RAM030A]
:DEF_EVAL
:ROMAE86    SET Z, 1
            SET [VALTYP], 0
:ROMAE8A    JSR CHRGET
            IFN C, 0
                JMP ROMAE92
:ROMAE8F    JMP FIN
            
:ROMAE92    JSR ROMB113
            IFN C, 0
                JMP ROMAE9A
            JMP ROMAF28
:ROMAE9A
            IFN A, 0xFF ; PI character
                JMP ROMAEAD
            SET A, PIVAL
            JSR MOVFM
            JMP CHRGET

; float value of PI
:PIVAL
:ROMAEA8    DAT 0x82, 0x490F, 0xDAA1

:ROMAEAD    SET C, 0
        IFE A, 0x2E ; Decimal point
                JMP ROMAE8F
            IFE A, 0xAB ; plus code
                JMP ROMAF0D
            IFE A, 0xAA ; times code
                JMP ROMAE8A
            IFN A, CBM_QUOTE ; quote mark
                JMP ROMAECC
            IFG A, CBM_QUOTE ; carry over from 6502 CMP above
                SET C, 1 
