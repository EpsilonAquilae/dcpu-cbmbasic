; main loop
:MAIN
:ROMA480    JMP [IMAIN] ; normally ROMA483
:DEF_MAIN
:ROMA483    JSR INLIN ; address of line input in X
            SET [RAM7A], X
:ROMA48A    JSR CHRGET     ; JSR $0073
:ROMA48D    SET X, A ; TAX
:ROMA48E    IFE A, 0  ; BEQ $A480 
