:FAKEBDCD   SET I, 0
:BDCDLOOP   SET B, A
            MOD B, 10
            ADD B, 0x30
            STI [NUMBER+I], B
            DIV A, 10
            IFG A, 0
                JMP BDCDLOOP
            SET [NUMBER+I], 0
:BDCDOUTLOOP
            STD A, [NUMBER-1+I]
            SET PUSH, I
