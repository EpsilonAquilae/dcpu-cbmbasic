; small test program that can be run instead of BASIC
; to test specific things.  Also useful for catching
; routines that don't establish pointers properly and
; start destroying low memory.
:TESTPRGINIT ;IAQ 1
            SET A, CBM_CLR 
            JSR CHROUT
:TESTPRG    SET A, 38911
            JSR LINPRT
            SET A, 38911
            JSR LINPRT
            SET A, 32
            JSR CHROUT
            SET A, 6502
            JSR LINPRT
            SET A, CBM_RETURN
            JSR CHROUT
             ; skip shift check
            JMP GETATHING

            SET A, 2 ; check for key
            SET B, 0x90 ; SHIFT
            HWI [KEYBOARD]
            IFE C, 1 ; if pressed
                JMP GETATHING
            SET C, 0
            SET B, WORDS
:TESTLOOP   SET A, [B]
            IFE A, 0
               JMP TESTPRG
            JSR CHROUT
            ADD B, 1
            JMP TESTLOOP
            JMP TESTPRG
            ;dat 0 ; abort emulator

:HALT SET [SCREENMEM], 0x4C21
      JMP HALT


:GETATHING
    SET [TESTLEN], 0
:GETATHINGLOOP
    JSR CHRIN
    ADD [TESTLEN], 1
    IFN A, CBM_RETURN 
       JMP GETATHINGLOOP
    SET A, CBM_RETURN
    JSR CHROUT
    ;; display number of length
    SET A, [TESTLEN]
    SUB A, 1
    JSR LINPRT
    SET A, CBM_RETURN
    JSR CHROUT
    JMP GETATHING

:TESTLEN
    DAT 0

    
