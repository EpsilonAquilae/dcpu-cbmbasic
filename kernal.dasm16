            SET PUSH, [INTFLG]
            SET PUSH, [VALTYP]
            JSR FRMEVAL
            SET A, POP
            MUL A, 2  ; ROL
            AND A, 0xFF
            JSR ROMAD90
            ; WHAT?
            SET EX,0x9999
            JMP CRASH
:ROMA9C1    SET A, POP
:ROMA9C2    IFC A, 0x80 ; BPL
                JMP ROMA9D6

; assign to integer
