; get next non-string value
:FRMNUM
:ROMAD8A    JSR FRMEVAL
:ROMAD8D    SET C, 0
            JMP ROMAD90
; check value to be string
:ROMAD8F    SET C, 1
; check value according to C flag
:ROMAD90    IFB [VALTYP], 0x80
                JMP ROMAD97
            IFE C, 1
                JMP ROMAD99
            RTS
:ROMAD97    IFE C, 1
                RTS
:ROMAD99    SET X, 0x16  ; ?TYPE MISMATCH ERROR
            JMP ERROR

                    
; evaluate formula
:FRMEVAL
:ROMAD9E    SUB [RAM7A], 1
            SET X, 0
            JMP ROMADAA
:ROMADA9    SET PUSH, A
:ROMADAA    SET PUSH, X
            SET A, 1
            JSR GETSTK
            JSR EVAL ; do math on a term
            SET [OPMASK], 0
:ROMADB8    JSR CHRGOT
:ROMADBB
:ROMADBC    SUB A, 0xB1 ; code for greater-than
            IFL A, 0
                JMP ROMADD7
            IFG A, 2         ;CMP #3
                JMP ROMADD7  ;BCS $ADD7
            SHL A, 1         ;CMP #$01
            IFL A, 1         ;ROL
                BOR A, 0x01  ;EOR #$01
            XOR A, [OPMASK]
            IFL A, [OPMASK]
                JMP ROMAE30
            SET [OPMASK], A
            JSR CHRGET
            JMP ROMADBB
:ROMADD7    SET X, [OPMASK]
            IFE X, 0
                JMP ROMAE07
            IFG C, 0
            ADD A, 7
            IFL A, 0x100
                JMP ROMAE58
            ADD A, CBM_RETURN
            IFB A, 0xFF  ; nonzero in 8-bit
                JMP ROMADE8
            JMP ROMB63D
:ROMADE8    ; originally ADC #$FF did something
            IFC A, 0x100 ; Carry bit clear in original
                SUB A, 1
            SET [INDEX], A
            SHL A, 1
            ADD [INDEX], A
            SET Y, A
            ; token was...
:ROMADF0    SET A, POP
            IFL [ROMA080+Y], A
                JMP ROMAE5D
            JSR ROMAD8D
:ROMADF9    SET PUSH, A
:ROMADFA    JSR ROMAE20
:ROMADFD    SET A, POP
            IFC [RAM4B], 0x80  ; positive in 8-bit
                JMP ROMAE19
            SET X, A
            IFE X, 0
                JMP ROMAE5B
            JMP ROMAE66         

:ROMAE07    ASR [RAM0D], 1
            SET C, EX
            SET A, X
            SHL A, 1
            BOR A, C
:ROMAE08    SUB [RAM7A], 1
            SET Y, 28   ; was 27 bytes, now 18 words 
            SET [RAM4D], A  ; ?
            JMP ROMADF0
            IFL A, [ROMA080+Y]
                JMP ROMADF9
            JMP ROMAE66
            
:ROMAE19
; recursive entry for evaluation of expression
:ROMAE20    
            SET PUSH, [ROMA081+Y]
            JSR ROMAE33
            SET A, [RAM4D]
            JMP ROMADA9
            
:ROMAE30    JMP SNERR

; push FAC onto stack
:ROMAE33
            SET EX, 0xAE33
            JMP CRASH

; APPLY OPERATOR
:ROMAE58
            SET Y, -1
            SET A, POP
:ROMAE5B    IFE A, 0
                JMP ROMAE80
:ROMAE5D    IFE A, 0x64   ; lowercase D?
                JMP ROMAE64
            JSR ROMAD8D ; This overflowed on sunday?
:ROMAE64    SET [RAM4B], Y
:ROMAE66    SET A, POP
            SHR A, 1
            SET [RAM12], A
            SET [ARGEXP], POP ; exponent of ARG
            SET [ARGHO], POP ; high mantissa of ARG
            SET [ARGHO+1], POP ; low mantissa of ARG
            SET A, POP
            SET [ARGSGN], A
            XOR A, [FACSGN]
            SET [ARISGN], A
:ROMAE80    SET A, [FACEXP]
:ROMAE82    RTS

; get arithmetic element routine            
; convert a single numeric term from 
