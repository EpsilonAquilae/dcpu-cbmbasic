                JMP [ROMAC4A]
            JSR ROMAB45 ; print question mark
:ROMAC4A    JSR ROMABF9 ; get line into input buffer
:ROMAC4D    SET [RAM7A], X
:ROMAC51    JSR CHRGET
            IFC [RAM0D], 0x80 ; BPL
                JMP ROMAC89
            IFC [INPFLG], 0x40 ; BVC
                JMP ROMAC65
:ROMAC5C    ADD X, 1
            SET [RAM7A], X
            SET [RAM07], A
            JMP ROMAC71
:ROMAC65    SET [RAM07], A
            SET C, 1 ; add 1 to address if quote matches
            IFE A, CBM_QUOTE
                JMP ROMAC72
            SET [RAM07], CBM_COLON
            SET A, CBM_COMMA
:ROMAC71    SET C, 0
:ROMAC72    SET [ENDCHR], A
            SET A, [RAM7A]
            ADD A, C
            JSR ROMB48D ; STRLIT without quotes?
            JSR ROMB7E2
            JSR ROMA9DA
            JMP ROMAC91
:ROMAC89    JSR FIN ; convert string to float
            SET A, [RAM0E]
            JSR ROMA9C2
:ROMAC91    JSR CHRGOT
            IFE Z, 1
                JMP ROMAC9D
            IFE A, CBM_COMMA
                JMP ROMAC9D
            JMP ROMAB4D
:ROMAC9D    SET [RAM43], [RAM7A]
            SET [RAM7A], [RAM4B]
            JSR CHRGOT
            IFE Z, 1
                JMP ROMACDF
            JSR ROMAEFD
            JMP ROMAC15

:ROMACB8    JSR ROMA906
            ADD Y, 1
            SET X, A
            IFN A, 0
                JMP ROMACD1
:ROMACBF    SET X, 0x0D ; error number
        ; todo: finish this, the Y thing need juggline
            JMP ERROR 
        
:ROMACD1    JSR ROMA8FB
            JSR CHRGOT
            SET X, A
            IFN X, 0x83 ; DATA code
                JMP ROMAC51
:ROMACDF    SET [RAM43], A
            SET X, [INPFLG]
