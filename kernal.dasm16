; GOSUB command

:STMT_GOSUB
:ROMA883    SET A, 3
            JSR GETSTK    ; check for room in the stack
            ; originally pushed 5 bytes to stack,
            ; now pushes 3 words
            SET PUSH, [RAM7A]
            SET PUSH, [CURLIN]
            SET PUSH, BASIC_GOSUB_TOKEN  ; to signify a GOSUB

:ROMA897    JSR CHRGOT

            JSR STMT_GOTO
            JMP NEWSTT  ; execute next statement

; GOTO command

:STMT_GOTO
:ROMA8A0    JSR LINGET  ; get target line
            JSR ROMA909  ; Set I to end of this line
            SET A, I ; start at next line
            IFG [CURLIN], [LINNUM]
                SET A, [TXTTAB] ; start at beginning
            JSR FINDLNA ; set [RAM5F] to address of target
            IFE C, 0 ; BCC $A8E3
                JMP ROMA8E3 ; throw ?UNDEF'D STATEMENT
:ROMA8C5    SET A, [RAM5F]
            SUB A, 1
:ROMA8C9    SET [RAM7A], A  ; got new location
            RTS

; RETURN command
:STMT_RETURN
:ROMA8D2   IFE Z, 0
                RTS
:ROMA8D4   SET [RAM4A], -1          ; LDA #$FF
:ROMA8D6                            ; STA $4A
:ROMA8D8   JSR FNDFOR   ; Search for FOR blocks on stack
           ; NOW:
:ROMA8DB   SET SP, X          ; TXS
:ROMA8DC    IFE A, BASIC_GOSUB_TOKEN ; CMP #$8D   ; GOSUB
:ROMA8DE        JMP ROMA8EB
:ROMA8E0    SET X, 0x0C  ; ?RETURN WITHOUT GOSUB
:ROMA8E2        JMP ERROR
:ROMA8E3    SET X, 0x11 ; ?undef'd statment
:ROMA8E5        JMP ERROR

; alternate entry for syntax error
