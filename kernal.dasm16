            SET X, [RAM64]
            ;SET Y, [RAM62] ; 63
            ;SET A, [RAM65]
            JMP SETTIM

; assign to variable string
:ROMAA2C    SET Y, [RAM64]
            ; originally compared two bytes
            IFL [Y+1], [FREETOP]; beginning of string variable area
                JMP ROMAA4B
            IFL Y, [VARTAB] ; beginning of variable area
                JMP ROMAA4B
            JMP ROMAA52
:ROMAA4B    SET A, [RAM64]
                JMP ROMAA68
:ROMAA52    SET A, [Y]
            JSR ROMB475
            SET [RAM6F], [RAM50]
            JSR MOVINS  ; move string
            SET A, FACEXP
; move descriptor into variable
:ROMAA68    SET [RAM50], A
            JSR FRETMS ; herp derp stack
            SET A, [RAM50]
            SET Y, [RAM49]
            SET [Y], [A]
            SET [Y+1], [A+1]
            SET [Y+2], [A+2]
            RTS

; add next digit to float accum

:ROMAA1D    SET A, [INDEX]
            ADD A, Y
            SET A, [A]  ; LDA ($22),Y
            JSR RAM0080 ; verify is digit
            IFN C, 0 ; not a digit
                JMP FCERR
            SUB A, 0x30
:ROMAA29    JMP FINLOG


