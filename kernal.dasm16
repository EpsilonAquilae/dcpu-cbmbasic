            ADD X, 1
            SET [X], A
            IFC X, 0xFF
                JMP ROMBB7A
            IFC X, 0x80 
                JMP ROMBB7E  ; Store to [FACOV]
            SET A, 1
:ROMBB4C    SET EX, POP
:ROMBB4D    IFN EX, 0
                JMP ROMBB5D
:ROMBB4F    SHL [ARGHO], 1
            SET C, EX
