:FOUTBL
:ROMBF16   DAT 0xFA0A,0x1F00    ;leftmost digit
:ROMBF1A   DAT 0x0098,0x9680    ;
:ROMBF1E   DAT 0xFFF0,0xBDC0    ;
:ROMBF22   DAT 0x0001,0x86A0    ;
:ROMBF26   DAT 0xFFFF,0xD8F0    ;
