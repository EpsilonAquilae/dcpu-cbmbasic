;assign to string
:ROMA9D9    SET A, POP ; PLA
:ROMA9DA    SET Y, [RAM4A]
            IFN Y, 0xBF ; ???
                JMP ROMAA2C
            ;assign to TI$
            JSR ROMB6A6
            IFN A, 6
                JMP FCERR
            SET Y, 0
            SET [RAM61], 0
            SET [RAM66], 0
:ROMA9ED    SET [RAM71], Y
            JSR ROMAA1D
            JSR MUL10
            ADD [RAM71], 1
            SET Y, [RAM71]
            JSR ROMAA1D
            JSR MOVAF
            SET X, A
            IFE A, 0
                JMP ROMAA07
